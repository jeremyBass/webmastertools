<config>
    <modules>
        <Wsu_GATracking>
            <version>1.6.0.0</version>
        </Wsu_GATracking>
    </modules>
    <global>
        <models>
            <gatracking>
                <class>Wsu_GATracking_Model</class>
            </gatracking>
        </models>
        <blocks>
            <gatracking>
                <class>Wsu_GATracking_Block</class>
            </gatracking>
        </blocks>
        <helpers>
            <gatracking>
                <class>Wsu_GATracking_Helper</class>
            </gatracking>
        </helpers>
    </global>
    <frontend>
        <translate>
            <modules>
                <Wsu_GATracking>
                    <files>
                        <default>Wsu_GATracking.csv</default>
                    </files>
                </Wsu_GATracking>
            </modules>
        </translate>
        <events>
            <checkout_onepage_controller_success_action>
                <observers>
                    <gatracking_order_success>
                        <class>gatracking/observer</class>
                        <method>setGATrackingOnOrderSuccessPageView</method>
                    </gatracking_order_success>
                </observers>
            </checkout_onepage_controller_success_action>
            <checkout_multishipping_controller_success_action>
                <observers>
                    <gatracking_order_success>
                        <class>gatracking/observer</class>
                        <method>setGATrackingOnOrderSuccessPageView</method>
                    </gatracking_order_success>
                </observers>
            </checkout_multishipping_controller_success_action>
            <googlecheckout_block_link_html_before>
                <observers>
                    <gatracking_googlecheckout_integration>
                        <class>gatracking/observer</class>
                        <method>injectAnalyticsInGoogleCheckoutLink</method>
                    </gatracking_googlecheckout_integration>
                </observers>
            </googlecheckout_block_link_html_before>
        </events>
        <layout>
            <updates>
                <gatracking>
                    <file>gatracking.xml</file>
                </gatracking>
            </updates>
        </layout>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <Wsu_GATracking>
                    <files>
                        <default>Wsu_GATracking.csv</default>
                    </files>
                </Wsu_GATracking>
            </modules>
        </translate>
    </adminhtml>
</config>
